{% extends 'base.html.twig' %}

{% block body %}

    {% for ad in ads %}
        <div>
            <span>{{ ad.title }}</span> |
            <span>{{ ad.city.name }}</span> |
            <span>{{ ad.category.name }}</span> |
            <span>{{ ad.user.email }}</span>
        </div>
        <br />
    {% endfor %}

{% endblock %}